    "ava": "^0.11.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "jsdom": "^8.0.1",
    "minidom": "^1.0.0",
    "semistandard": "^7.0.5",
    "snazzy": "^2.0.1"
  },
